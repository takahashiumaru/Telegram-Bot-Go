{
  "version": 2,
  "builds": [
    {
      "src": "./main.go",
      "use": "@vercel/go",
      "config": {
        "goVersion": "1.19.13",
        "exportName": "handler",
        "entryPoint": "main"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./main.go"
    }
  ]
}

